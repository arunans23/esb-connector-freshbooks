<component name="freshbooks-expense" type="synapse/template">
   <subComponents>
      <component name="createExpense">
         <file>createExpense.xml</file>
         <description>Create a new expense.</description>
      </component>
      <component name="updateExpense">
         <file>updateExpense.xml</file>
         <description>Update an existing expense.</description>
      </component>
      <component name="getExpense">
         <file>getExpense.xml</file>
         <description>Return the complete expense details.</description>
      </component>
      <component name="deleteExpense">
         <file>deleteExpense.xml</file>
         <description>Delete an existing expense.</description>
      </component>
      <component name="listExpense">
         <file>listExpense.xml</file>
         <description>Returns a list of expense summaries.</description>
      </component>
   </subComponents>
</component>