<component name="freshbooks-tax" type="synapse/template">
   <subComponents>
      <component name="createTax">
         <file>createTax.xml</file>
         <description>Create a new tax.</description>
      </component>
      <component name="updateTax">
         <file>updateTax.xml</file>
         <description>Update an existing tax.</description>
      </component>
      <component name="getTax">
         <file>getTax.xml</file>
         <description>Return the complete details for a given tax.</description>
      </component>
      <component name="listTaxes">
         <file>listTaxes.xml</file>
         <description>Returns a list of taxes.</description>
      </component>
      <component name="deleteTax">
         <file>deleteTax.xml</file>
         <description>Delete an existing tax.</description>
      </component>
   </subComponents>
</component>